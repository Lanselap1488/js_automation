const fs = require('fs-extra')
fs.ensureDirSync('./kek/')

fs.ensureFileSync('./kek/file')

fs.ensureDirSync('./folder/')

fs.moveSync('./kek/file/','./folder/file/')

fs.removeSync('./folder/')

fs.removeSync('./kek/')

